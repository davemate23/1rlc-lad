%h1 Listing Competencies
= render 'shared/search/search', resourse_url: employee_competencies_path(@parent), filters: [:created_at, :updated_at, :start_date, :end_date, :competency_type, :grade], columns: ['employee', 'competency_type', 'grade', 'start_date', 'end_date']
